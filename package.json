{
	"name": "nanothread",
	"author": "snuffydev",
	"license": "ISC",
	"version": "0.1.0",
	"type": "module",
	"source": "./src/index.ts",
	"description": "A tiny Cross-Platform Worker & Concurrency Library",
	"keywords": [
		"threads",
		"worker",
		"concurrency",
		"async",
		"threading",
		"browser",
		"mutex"
	],
	"files": [
		"dist",
		"package.json"
	],
	"main": "./dist/index-node.js",
	"scripts": {
		"build": "tsup-node",
		"start": "pnpm run build && node --loader=ts-node/esm --experimental-specifier-resolution=node ./index",
		"test": "size-limit"
	},
	"exports": {
		".": {
			"default": "./dist/index-browser.js",
			"browser": "./dist/index-browser.js",
			"import": "./dist/index-node.cjs",
			"require": "./dist/index-node.js",
			"types": "./dist/index-browser.d.ts"
		},
		"./web": {
			"default": "./dist/index-browser.js",
			"browser": "./dist/index-browser.js",
			"import": "./dist/index-browser.cjs",
			"require": "./dist/index-browser.js",
			"types": "./dist/index-browser.d.ts"
		},
		"./sync": {
			"browser": "./dist/sync/index.js",
			"default": "./dist/index-browser.js",
			"import": "./dist/sync/index.cjs",
			"require": "./dist/sync/index.js",
			"types": "./dist/sync/index.d.ts"
		},
		"./models": {
			"default": "./dist/index-browser.js",
			"browser": "./dist/models/index.js",
			"import": "./dist/models/index.cjs",
			"require": "./dist/models/index.js",
			"types": "./dist/models/index.d.ts"
		},
		"./package.json": "./package.json"
	},
	"size-limit": [
		{
			"limit": "1 KB",
			"import": "*",
			"path": "src/index.ts"
		}
	],
	"devDependencies": {
		"@size-limit/preset-small-lib": "^8.1.0",
		"@types/node": "^18.8.3",
		"size-limit": "^8.1.0",
		"typescript": "^4.6.4"
	}
}